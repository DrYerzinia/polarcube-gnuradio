<?xml version="1.0"?>
<block>
  <name>UDP PDU Receiver</name>
  <key>spacegrant_udp_pdu_receiver</key>
  <category>spacegrant</category>
  <import>import spacegrant</import>
  <make>spacegrant.udp_pdu_receiver($ipaddr, $port)</make>

	<param>
		<name>Destination IP Address</name>
		<key>ipaddr</key>
		<type>string</type>
	</param>

	<param>
		<name>Destination Port</name>
		<key>port</key>
		<type>int</type>
  </param>

	<sink>
		<name>pdu</name>
		<type>message</type>
  </sink>

  </block>
