<?xml version="1.0"?>
<block>
  <name>Sat Doppler Correction</name>
  <key>spaegrant_sat_doppler_correction</key>
  <category>spacegrant</category>
  <import>import spacegrant</import>
  <make>spacegrant.sat_doppler_correction(self.set_$target(), $sat_freq(), "$tle1()", "$tle2()", "$longitude()", "$latitude()", $elevation())</make>

  <param>
    <name>Variable</name>
    <key>target</key>
    <value>freq</value>
    <type>string</type>
  </param>

  <param>
    <name>Sat Freq</name>
    <key>sat_freq</key>
    <type>int</type>
  </param>

  <param>
    <name>TLE1</name>
    <key>tle1</key>
    <type>string</type>
  </param>

  <param>
    <name>TLE2</name>
    <key>tle2</key>
    <type>string</type>
  </param>

  <param>
    <name>Longitude</name>
    <key>longitude</key>
    <type>string</type>
  </param>

  <param>
    <name>Latitude</name>
    <key>latitude</key>
    <type>string</type>
  </param>

  <param>
    <name>Elevation</name>
    <key>elevation</key>
    <type>int</type>
  </param>


</block>
